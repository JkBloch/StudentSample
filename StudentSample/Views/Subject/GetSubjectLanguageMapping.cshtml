@{
    ViewData["Title"] = "Subject Detail " + Model.SubjectName;
}
@model SubjectLanguageMappingModel
<div class="container">
    <h3 class="h3">Subject Language Mapping</h3>
    <div class="row">
        <div class="col-md-6">
            <hr />
            <ul class="list-group">
                <li class="list-group-item"><span class="fw-bold">Subject - </span> @Model.SubjectName</li>
                <li class="list-group-item"><span class="fw-bold">Languge - </span> @Model.LanguageName</li>
            </ul>
        </div>
    </div>
</div>